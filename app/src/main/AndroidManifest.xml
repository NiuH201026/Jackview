        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.think.finance.SVGActivity"></activity>
        <activity android:name="org.think.finance.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.think.finance.LottoViewActivity" />
        <activity android:name="org.think.finance.LuckyDrawActivity" />
        <activity android:name="org.think.finance.CoinViewActivity" />
        <activity android:name="org.think.finance.CheckInActivity" />
        <activity android:name="org.think.finance.SportActivity" />
        <activity android:name="org.think.finance.CoinRainActivity" />
        <activity android:name="org.think.finance.BerizActivity" />
    </application>

</manifest>